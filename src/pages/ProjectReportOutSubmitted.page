<apex:page title="Project report out" docType="html-5.0"
    controller="ProjectReportOutController" action="{!getPRO}">
<head>
    <style type="text/css">
        .pro-label {
            font-weight: bold;
        }
    </style>
</head>
<body>
    Submission Successful!<br/><br/>
    An email has been sent to you and everyone on the project report out email list for this project. <br/><br/>
    
    <apex:outputLink target="_blank" value="https://confluence.dimagi.com/display/internal/Project+Report+Outs">https://confluence.dimagi.com/display/internal/Project+Report+Outs</apex:outputLink>
    <br/><br/>
    <apex:PageBlock >
        <apex:form >
            <apex:pageBlockSection >
                <apex:pageBlockSectionItem >
                    <apex:commandButton action="{!submitAnother}" value="  Submit Another One  "></apex:commandButton>
                </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
        </apex:form>
    </apex:PageBlock>

    <apex:PageBlock >
        <b>Report Out Name:</b> {!ProObj.Name}<br/><br/>
        <b>Resource Planning:</b> {!ProObj.Resource_Planning__c}<br/><br/>
        <b>Overall Status:</b> {!ProObj.status__C}<br/><br/>
        <b>Ability to meet Schedule:</b> {!ProObj.Ability_to_meet_Schedule__c}<br/><br/>
        <b>Ability to stay within Scope:</b> {!ProObj.Ability_to_stay_within_Scope__c}<br/><br/>
        <b>Current System Usage:</b> {!ProObj.Current_System_Usage__c}<br/><br/>
        <!--<b>Custom Reports/Development:</b> {!ProObj.Custom_Reports_Development__c}<br/><br/>-->
        <b>Partner Relationship:</b> {!ProObj.Partner_Relationship__c}<br/><br/><br/>
        
        <b>Key Accomplishments (last 2 weeks)</b><br/>
        <apex:outputText escape="false" value="{!ProObj.Key_Accomplishments_Last_Two_Weeks__c}"/><br/><br/>
        <b>Key Next Steps (next 2 weeks)</b><br/>
        <apex:outputText escape="false" value="{!ProObj.Key_Next_Steps_next_2_weeks__c}"/><br/><br/>
        <b>Issues / Questions / Notables</b><br/>
        <apex:outputText escape="false" value="{!ProObj.Issues_Questions_Notables__c}"/><br/><br/>
        <b>Upcoming Important Project / Invoicing Deadlines</b><br/>
        <apex:outputText escape="false" value="{!ProObj.Client_Facing__c}"/><br/><br/>
        
                
        <apex:outputPanel rendered="{!showDevWork}">
            <b>Requires Developer Work</b><br/>
            <apex:outputText escape="false" value="{!ProObj.Requires_Developer_Work__c}"/><br/><br/>
        </apex:outputPanel>
        
        <b>Project Dashboard:</b> {!ProObj.Project_Dashboard__c}<br/><br/>        
        <b>% Deliverables Completed:</b> <apex:outputText value="{!proTabContractPercntgethroughSOW}"/>  <br/><br/>        
        <b>Execution Efficiency:</b> <apex:outputText value="{!proTabContract.Execution_Efficiency__c}"/><br/><br/>        
        
    </apex:PageBlock>
</body>
</apex:page>