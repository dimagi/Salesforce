                        <apex:actionFunction status="loadingDiv" action="{!tablePagination.resetPageNumber}" name="resetPageNumStatus" reRender="pagination-panel,projectSpaceDataTable" />
                    </apex:outputPanel>
                 </apex:pageBlock>
                 <br/>
                 <apex:pageBlock title="Project Spaces" id="projectSpaces" >
                    <apex:outputPanel id="graph-container">
                        <div id="domains-graph-navigation" class="graph-menu">
                            <apex:outputLabel value="From : " for="graph-from"/>
                            <apex:input id="graph-from" value="{!graphFrom}" type="date" />

                            <apex:outputLabel value="To : " for="graph-to"/>
                            <apex:input id="graph-to" value="{!graphTo}" type="date" />

                            <apex:outputLabel value="Interval : " for="graph-interval"/>
                            <apex:selectList id="graph-interval" value="{!graphInterval}" size="1" multiselect="false">
                                <apex:selectOption itemValue="daily" itemLabel="Daily"/>
                                <apex:selectOption itemValue="monthly" itemLabel="Monthly"/>
                                <apex:selectOption itemValue="yearly" itemLabel="Yearly"/>
                            </apex:selectList>

                            <apex:outputLabel value="Date field : " for="graph-field"/>
                            <apex:selectList id="graph-field" value="{!graphField}" size="1" multiselect="false">
                                <apex:selectOption itemValue="Domain_date_created_date_wrapper__c" itemLabel="Date Created"/>
                                <apex:selectOption itemValue="Domain_first_form_sub_date_wrapper__c" itemLabel="First Form Submission"/>
                                <apex:selectOption itemValue="Domain_last_form_sub_date_wrapper__c" itemLabel="Last Form Submission"/>
                            </apex:selectList>

                            <input type="button" class="btn" onclick="applyGraphData();" value="Apply" /><br/>
                            <apex:actionFunction action="{!reloadGraphData}" name="reloadGraphDataJs" status="loadingDiv"
                                         reRender="domains-graph-navigation,domain-graph-data,test-panel-id,graph-from,graph-to" oncomplete="updatePageElements();" />
                        </div>
                        <br/>
                        <apex:inputHidden id="domain-graph-data" value="{!graphDataJSON}" />
                        <br/>
                        <div id="tabs">
                            <ul>
                                <li><a href="#tabs-1">Bar Chart</a></li>
                                <li><a href="#tabs-2">Cumulative Chart</a></li>
                            </ul>
                            <div id="tabs-1">
                                <div id="domains-graph" class="domains-graph"></div>
                            </div>
                            <div id="tabs-2">
                                <div id="domains-graph-cumulative" class="domains-graph"></div>
                            </div>
                        </div>
                    </apex:outputPanel>
                </apex:pageBlock>
            </div>
        </apex:outputPanel>
    </apex:form>
</apex:page>