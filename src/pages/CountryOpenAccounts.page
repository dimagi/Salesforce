<apex:page standardController="Country__c" extensions="CountryAccounts">
	<apex:pageBlock title="My Accounts">
		<apex:pageBlockTable value="{!OpenAccounts}" var="a">
			<apex:column value="{!a.name}" />
			<apex:column value="{!a.Id}" />
			<apex:column value="{!a.Phone}" />
			<apex:column value="{!a.Country__c}" />
		</apex:pageBlockTable>
	</apex:pageBlock>

</apex:page>